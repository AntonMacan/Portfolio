@model Koncesionari

@{
    ViewBag.Title = "Novi koncesionar";
}

<h2>Unos novog koncesionara</h2>

<form asp-action="Create" method="post">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label asp-for="NazivKoncesionara"></label>
        <div><span asp-validation-for="NazivKoncesionara" class="text-danger" /></div>
        <input asp-for="NazivKoncesionara" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Url"></label>
        <div><span asp-validation-for="Url" class="text-danger" /></div>
        <input asp-for="Url" class="form-control" />
    </div>
    <div class="text-center">
        <button class="btn btn-primary" type="submit">Dodaj</button>
        <a asp-action="Index" class="btn btn-secondary">Odustani</a>
    </div>
</form>

@section scripts {
    <partial name="IncludeValidation" />
}