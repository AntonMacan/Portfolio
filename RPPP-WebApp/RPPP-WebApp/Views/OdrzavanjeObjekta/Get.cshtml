@model OdrzavanjeObjektaViewModel

<tr hx-target="this" hx-swap="outerHTML">
    <td class="text-left">@Model.Id</td>
    <td class="text-left">@Model.NazivObjekta</td>
    <td class="text-left">@Model.datum</td>
    <td class="text-left">@Model.Opis</td>
    <td class="text-left">@Model.Ishod</td>
    <td class="text-left">@Model.TipNaziv</td>
    <td class="text-left">@Model.Odrzavatelj</td>
    <td>
        <a class="btn btn-sm"
           hx-get="@Url.Action(nameof(OdrzavanjeObjektaController.Edit), new { id = Model.Id })"
           title="Ažuriraj"><i class="fas fa-edit"></i></a>
    </td>
    <td>
        <button class="btn btn-sm btn-danger"
                hx-confirm="Obrisati mjesto?"
                hx-delete="@Url.Action(nameof(OdrzavanjeObjektaController.Delete), new { id = Model.Id})"
                title="Obriši">
            <i class="fas fa-trash-alt"></i>
        </button>
    </td>
</tr>